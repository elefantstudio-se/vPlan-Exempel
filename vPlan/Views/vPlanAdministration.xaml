<Page
    x:Class="vPlan.vPlanAdministration"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:Behaviors="using:Template10.Behaviors"
      xmlns:Core="using:Microsoft.Xaml.Interactions.Core"
      xmlns:Interactivity="using:Microsoft.Xaml.Interactivity"
      xmlns:controls="using:Template10.Controls"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="using:vPlan.Views"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:vm="using:vPlan.ViewModels" x:Name="KalendersPage"
      mc:Ignorable="d">

    <Grid Background="White" Margin="0,0,-291,-244">
        <!--  header  -->
        <controls:PageHeader Frame="{x:Bind Frame}" Text="vPlan Administration" Background="#FFE2553B" FontFamily="Open Sans" >
            <AppBarButton x:Name="SearchButton" Grid.Column="1" Command="{Binding SearchCommand}">
                <SymbolIcon Symbol="Find" />
            </AppBarButton>
        </controls:PageHeader>
    </Grid>
</Page>
